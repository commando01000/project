CREATE TABLE `project`.`users` ( `id` INT NOT NULL AUTO_INCREMENT , `name` VARCHAR(100) NOT NULL , `gender` ENUM('male','female','other') NOT NULL , `age` INT(2) NOT NULL , `job` VARCHAR(100) NOT NULL , `marital status` ENUM('single','married','engaged') NOT NULL , `number in family` INT(2) NOT NULL , `address` VARCHAR(300) NOT NULL , `phone no` INT(11) NOT NULL , `home no` INT(8) NOT NULL , `supervised by` VARCHAR(100) NOT NULL , `complain` VARCHAR(300) NOT NULL , PRIMARY KEY (`id`)) ENGINE = MyISAM;
